{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "ouvrir-societe-hong-kong.fr"
        }
      ],
      "destination": "https://www.ouvrir-societe-hong-kong.fr/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
